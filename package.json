{
  "name": "climpire",
  "version": "1.1.9",
  "description": "HyperClaw - Coding/dev workflowâ€“focused AI orchestration. Command your CLI agent empire from the CEO desk.",
  "type": "module",
  "scripts": {
    "migrate:v1.0.5": "node scripts/auto-apply-v1.0.5.mjs",
    "predev": "node scripts/auto-apply-v1.0.5.mjs",
    "predev:local": "node scripts/auto-apply-v1.0.5.mjs",
    "predev:tailscale": "node scripts/auto-apply-v1.0.5.mjs",
    "prestart": "node scripts/auto-apply-v1.0.5.mjs",
    "prestart:tailscale": "node scripts/auto-apply-v1.0.5.mjs",
    "dev": "concurrently -n web,api -c cyan,magenta \"cross-env VITE_DEV=1 vite --host 0.0.0.0 --port 8800\" \"cross-env VITE_DEV=1 HOST=0.0.0.0 nodemon --watch server --ext ts --exec tsx server/index.ts\"",
    "dev:local": "concurrently -n web,api -c cyan,magenta \"node scripts/wait-for-api.mjs cross-env VITE_DEV=1 vite --host 127.0.0.1 --port 8800\" \"cross-env VITE_DEV=1 HOST=127.0.0.1 nodemon --watch server --ext ts --exec tsx server/index.ts\"",
    "dev:tailscale": "concurrently -n web,api -c cyan,magenta \"cross-env VITE_DEV=1 vite --host 0.0.0.0 --port 8800\" \"cross-env VITE_DEV=1 HOST=0.0.0.0 nodemon --watch server --ext ts --exec tsx server/index.ts\"",
    "start": "tsx server/index.ts",
    "start:tailscale": "cross-env HOST=0.0.0.0 tsx server/index.ts",
    "build": "tsc -b && vite build",
    "build:server": "node scripts/build-server.mjs",
    "electron": "electron .",
    "clean:release": "node scripts/clean-release.cjs",
    "download-node": "powershell -ExecutionPolicy Bypass -File scripts/download-node-for-electron.ps1",
    "dist:win": "pnpm run build && pnpm run build:server && pnpm run clean:release && cross-env CSC_IDENTITY_AUTO_DETECTION=false electron-builder --win --config electron-builder.config.cjs",
    "preview": "vite preview",
    "setup": "node scripts/setup.mjs",
    "arch:map": "node scripts/generate-architecture-report.mjs",
    "preflight:public": "bash scripts/preflight-public.sh",
    "audit:verify": "node scripts/verify-security-audit-log.mjs",
    "check:lines": "node scripts/check-line-count.mjs",
    "test:comm:llm": "node scripts/qa/llm-comm-test.mjs",
    "test:comm:oauth": "node scripts/qa/oauth-comm-test.mjs",
    "test:comm:api": "node scripts/qa/api-comm-test.mjs",
    "test:comm:suite": "node scripts/qa/run-comm-suite.mjs",
    "test:comm-status": "node scripts/test-comm-status.mjs",
    "test:qa:office:smoke": "node scripts/qa/office-console-smoke.mjs",
    "test:qa:office:perf": "node scripts/qa/office-performance-baseline.mjs",
    "test:qa:office:resolutions": "node scripts/qa/office-resolution-compare.mjs",
    "test:qa:office:management": "node scripts/qa/office-management-requirements.mjs",
    "test:qa:office:theme": "node scripts/qa/office-theme-requirements.mjs",
    "test:qa:project-path": "node scripts/qa/project-path-api-smoke.mjs",
    "test:web": "vitest --config vitest.config.ts",
    "test:api": "vitest --config server/vitest.config.ts",
    "test:e2e": "playwright test --config playwright.config.ts",
    "test": "pnpm run test:web && pnpm run test:api",
    "test:ci": "pnpm run test:web --coverage && pnpm run test:api --coverage && pnpm run test:e2e",
    "format": "prettier --write \"src/**/*.{ts,tsx}\" \"server/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\" \"server/**/*.ts\""
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@rollup/rollup-darwin-arm64": "^4.57.1",
    "cors": "^2.8.6",
    "express": "^5.2.1",
    "lightningcss-darwin-arm64": "^1.31.1",
    "lucide-react": "^0.511.0",
    "pixi.js": "^8.6.6",
    "pptxgenjs": "^3.12.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.6.2",
    "ws": "^8.19.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.0",
    "@tailwindcss/vite": "^4.1.8",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@types/supertest": "^6.0.3",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^3.0.3",
    "autoprefixer": "^10.4.21",
    "electron": "^33.0.0",
    "electron-builder": "^25.0.0",
    "esbuild": "^0.24.0",
    "concurrently": "^9.2.1",
    "cross-env": "^7.0.3",
    "jsdom": "^26.1.0",
    "msw": "^2.8.2",
    "nodemon": "^3.1.11",
    "playwright": "^1.58.2",
    "postcss": "^8.5.4",
    "sharp": "^0.34.5",
    "supertest": "^7.0.0",
    "tailwindcss": "^4.1.8",
    "tsx": "^4.21.0",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "vitest": "^3.0.3"
  },
  "engines": {
    "node": ">=22"
  },
  "main": "electron/main.cjs",
  "build": {
    "appId": "com.hyperclaw.app",
    "productName": "HyperClaw",
    "directories": { "output": "release" },
    "files": ["dist/**", "dist-server/**", "electron/**", ".env"],
    "win": { "target": ["nsis", "portable"], "signAndEditExecutable": false },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    },
    "asar": false
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
